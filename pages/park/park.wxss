/* pages/park/park.wxss */
.map{
  width: 100%;
  height: 500rpx;
  border: solid 1rpx #666666;
}
.headline{
  flex-wrap: nowrap;
  font-size: 14pt;
  color: #777;
  line-height: 110%;
  font-weight: bold;
  width: 100%;
  padding: 20rpx;
  background-color: #f3f3f3;
  margin: 5rpx 0rpx;
}
.park-block{
  width: 100%;
  height: 210rpx;
  background-color: #f3f3f3;
  font-size: 12pt;
  border: solid 2rpx #777777;
}
.text{
  font-size: 12pt;
  font-display: flex;
}
.icon-block{
  width: 50rpx;
  height: 50rpx;
 
}
.location-text{
  font-size: 14pt;
  font-weight: 100;
  
}